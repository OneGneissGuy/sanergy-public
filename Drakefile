
psql()
	psql -v ON_ERROR_STOP=1 -f $[INPUT] && touch $[OUTPUT]
        
PROFILE:=default_profile
%include $[PROFILE]

PYTHONPATH=$(echo $PWD:$PYTHONPATH)

; Sanergy provided files via Box.com; this zip archive is that folder.
Sanergy <- Sanergy.zip [-timecheck]
	unzip -o $INPUT

%include input/Drakefile
